node{
  stage('Github'){
    git 'https://github.com/sebsot/deploy'
  }

  stage('Analisis SonarQube'){
      def scannerHome = tool 'SonarScanner 4.0'
      withSonarQubeEnv('SonarQube Server') {
        sh "${scannerHome}/bin/sonar-scanner"
  }
}
}

