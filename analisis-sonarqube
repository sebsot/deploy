node{
  stage('Github'){
    git 'https://github.com/sebsot/deploy'
  }
  stage('Analisis SonarQube'){
    def mvnHome = tool name: 'Maven 3.9.5'
    echo ${mvnHome}
    dir('/ruta/del/proyecto'){
      sh 'mvn clean install sonar:sonar'
    }
  }
}

